{
    "version": 2,
    "builds": [
      {
        "src": "api/main.py",
        "functions": {
          "main": {
            "handler": "wsgi.handler",
            "runtime": "python3.9"
          }
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api"
      }
    ],
    "env": {
      "PYTHONPATH": "src"
    }
  }
  